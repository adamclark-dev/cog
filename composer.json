{
	"name": "message/cog",
	"description": "Message's powerful PHP5 framework for building modular systems",
	"keywords": ["framework", "cog"],
	"homepage": "http://message.co.uk",
	"authors": [
		{
			"name" : "The Message Development Team",
			"email": "dev@message.co.uk"
		}
	],
	"support": {
		"wiki"  : "http://server.local/wiki/projects/cog",
		"issues": "http://github.com/messagedigital/cog/issues"
	},
	"autoload": {
		"psr-4": {
			"Message\\Cog\\"      : "src",
			"Message\\Cog\\Test\\": "tests"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url" : "http://packages.message.co.uk"
		}
	],
	"require": {
		"php"          : ">=5.4.0",
		"ext-simplexml": "*",
		"ext-curl"     : "*",
		"ext-gd"       : "*",
		"ext-json"     : "*",
		"ext-mysqli"   : "*",
		"ext-intl"     : "*",

		"symfony/console"                  : "2.3.*",
		"symfony/event-dispatcher"         : "2.1.*",
		"symfony/form"                     : "2.3.*",
		"symfony/validator"                : "2.3.*",
		"symfony/http-kernel"              : "2.3.*",
		"symfony/process"                  : "2.1.*",
		"symfony/routing"                  : "2.3.*",
		"symfony/templating"               : "2.1.*",
		"symfony/translation"              : "2.3.*",
		"symfony/twig-bridge"              : "2.3.*",
		"symfony/twig-bundle"              : "2.3.*",
		"symfony/yaml"                     : "2.1.*",
		"symfony/filesystem"               : "2.3.*",
		"symfony/finder"                   : "2.2.*",
		"twig/twig"                        : "1.13.*",
		"pimple/pimple"                    : "~2.0",
		"monolog/monolog"                  : "1.7.*",
		"mtdowling/cron-expression"        : "1.0.*",
		"treasure-chest/treasure-chest"    : "0.1.*",
		"kriswallsmith/assetic"            : "1.1.*",
		"lmammino/jsmin4assetic"           : "1.0.*",
		"natxet/CssMin"                    : "3.0.*",
		"filp/whoops"                      : "1.0.*",
		"swiftmailer/swiftmailer"          : "~5.0",
		"knplabs/knp-snappy"               : "0.1.*",
		"message/wkhtmltopdf"              : "1.0.*",
		"itbz/fpdf"                        : "1.7.*",
		"itbz/fpdi"                        : "1.4.*",
		"dflydev/markdown"                 : "~1.0",
		"zendframework/zend-debug"         : "~2.2.6",
		"jeremeamia/superclosure"          : "~1.0"
	},
	"require-dev": {
		"mikey179/vfsStream"              : "1.1.*"
	},
	"suggest": {
		"ext-apc"                : "The preferred caching system in Cog",

		"message/cog-raven"      : "A Cog module that logs errors, exceptions and messages to Sentry"
	},
	"bin": [
		"bin/cog",
		"bin/cog-console"
	]
}
